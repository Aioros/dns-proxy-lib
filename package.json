{
  "name": "@aiorosdev/dns-proxy-lib",
  "version": "1.0.0",
  "description": "Simple DNS Proxy written in Node.JS. Override hosts, domains, or tlds.",
  "bin": {
    "dns-proxy": "run-server.js"
  },
  "main": "dns-proxy.js",
  "preferGlobal": true,
  "author": "Aioros",
  "license": "MIT",
  "repository": "aioros/dns-proxy-lib",
  "scripts": {
    "start": "node ./run-server.js",
    "test": "npm run test:standard",
    "test:standard": "standard *.js",
    "pkg": "pkg --out-dir release --targets node8-macos-x64,node8-alpine-x64,node8-linux-x64,node8-win run-server.js"
  },
  "dependencies": {
    "debug": "^4.0.0",
    "native-dns-packet": "~0.1.1",
    "rc": "^1.2.8",
    "wildcard2": "0.0.2"
  },
  "devDependencies": {
    "pkg": "5.5.2",
    "standard": "14.3.3"
  }
}
